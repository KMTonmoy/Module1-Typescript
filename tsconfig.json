{
  "explorer.confirmDelete": false,
  "security.workspace.trust.untrustedFiles": "open",
  "files.autoSaveDelay": 1,
  "files.autoSave": "afterDelay",
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontSize": 17,
  "editor.wordWrap": "on",
  "workbench.colorTheme": "Learn with Sumit Official Theme",
  "explorer.confirmPasteNative": false,
  "editor.stickyScroll.enabled": false,
  "editor.cursorBlinking": "smooth",
  "editor.cursorBlinkingInterval": 1000,
  "editor.fontFamily": "'Fira Code', 'JetBrains Mono', 'monospace'",
  "editor.fontLigatures": true,
  "editor.minimap.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.minimap.renderCharacters": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.codeLens": true,
  "editor.renderLineHighlight": "all",
  "editor.lineHighlightBackground": "#44475a",
  "editor.smoothScrolling": true,
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontFamily": "Cascadia Code, 'JetBrains Mono', monospace",
  "terminal.integrated.cursorBlinking": true,
  "explorer.compactFolders": false,
  "explorer.autoReveal": true,
  "breadcrumbs.enabled": true,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "always",
  "editor.autoSurround": "brackets",
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "powerMode.enabled": true,
  "powerMode.presets": "fireworks",
  "powerMode.combo.timeout": 1000,
  "powerMode.combo.threshold": 3,
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#4fb908"
  },
  "explorer.confirmDragAndDrop": false,
  "git.enableSmartCommit": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "glassit.alpha": 220,
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}